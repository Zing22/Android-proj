<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dragracing.dragracing">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/main_ac_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingActivity"
            android:label="@string/settings_ac_title" />
        <activity
            android:name=".RoomsActivity"
            android:label="@string/rooms_ac_title" />
        <activity
            android:name=".RoomActivity"
            android:label="@string/room_ac_title" />
        <activity
            android:name=".PlayActivity"
            android:label="@string/play_ac_title" />
        <activity android:name=".SRoomActivity" />
        <activity android:name=".SPlayActivity" />
        <activity android:name=".ReplayActivity" />
        <activity
            android:name=".ReplaysActivity"
            android:label="@string/replays_ac_title" />
        <activity android:name=".AboutActivity"></activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>